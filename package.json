  "bugs": {
    "url": "https://github.com/NR-inc/secret-diary-backend/issues"
  },
  "homepage": "https://github.com/NR-inc/secret-diary-backend#readme",
  "dependencies": {
    "@nexus/schema": "0.13.1",
    "@prisma/client": "^2.0.0-beta.3",
    "bcryptjs": "2.4.3",
    "dotenv": "8.2.0",
    "graphql": "14.6.0",
    "graphql-shield": "5.7.3",
    "graphql-yoga": "1.18.3",
    "jsonwebtoken": "8.5.1",
    "nexus": "0.12.0",
    "nexus-prisma": "0.12.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.0-beta.2",
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "8.3.9",
    "@types/node": "12.12.37",
    "@types/ws": "7.2.4",
    "ts-node": "8.9.0",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.8.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
